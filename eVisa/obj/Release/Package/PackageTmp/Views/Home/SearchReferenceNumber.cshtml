@{
    ViewBag.Title = "Search Reference Number";
}
<div class="content">
    <br />
    <div class="container">
        <ul>
            <li>For your convenience, now you can search your application reference number online</li>
            <li>Please enter your passport number and primary email address only</li>
        </ul>
        <label>Passport No</label>
        @Html.TextBox("PassportNo", "", new { @class = "form-control", required = "required", ng_model = "PassportNo", })
        <div>

        </div>
        <label>Primary Email Address</label>
        @Html.TextBox("PrimaryEmailAddress", "", new { @class = "form-control", required = "required", ng_model = "PrimaryEmailAddress", })
        <br />
        <button class="btn">
            <i class="fa fa-search"></i>
            Search
        </button>
    </div>
    <br />
</div>